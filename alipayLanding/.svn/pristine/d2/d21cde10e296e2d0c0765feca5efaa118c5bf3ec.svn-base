.orderDetail .topImg{
    padding: 2vw 4vw;
    height: 15.2vw;
    position: relative;
    margin-bottom:5vw;
}

.orderDetail .topImg .orderTop {
    width: 100%;
}

.orderDetail .vipBox{
    width: 88vw;
    padding: 3vw;
    background: rgb(255, 255, 255);
    margin: 0 auto 2.66667vw;
    border-radius: 2.66667vw;
    position: relative;
}

.orderDetail .vipBox:before {
    content: "";
    border: 2.66667vw solid rgba(0, 0, 0, 0);
    border-bottom: 2.66667vw solid rgb(255, 255, 255);
    position: absolute;
    top: -5.33333vw;
    left: 5%;
}

.orderDetail .vipBox .vipBanner .vipInfo {
    text-align: center;
}

.orderDetail .vipBox .vipBanner .iconBox {
    text-align: center;
    margin: 0 0 4.8vw;
}

.orderDetail .vipBox .vipBanner{
    padding: 4vw 0;
}

.vipBanner .iconBox image{
    width: 20vw;
    margin: 0 auto;
}
.orderDetail .vipBox .vipBanner .vipInfo .fontBig{
    font-size: 4.8vw;
    font-weight: 600;
    color: rgb(26, 26, 26);
}
.orderDetail .vipBox .vipBanner .vipInfo .fontSmall {
    font-size: 3.2vw;
    font-weight: 400;
    color: rgb(103, 103, 103);
    line-height: 4.8vw;
}
.vipBanner .vipInfo .info {
    width: 61.33333vw;
    margin: 2vw auto 2.53333vw auto;
    background-size: cover;
    border-radius: 1.33333vw;
    padding: 2.66667vw 1.33333vw 2.66667vw 9.33333vw;
    text-align: left;
}

.downLoad .notice {
    width:80vw;
    margin:0px auto;
}

.downLoad .notice .tips{
    font-size: 3.2vw;
    color: rgb(153, 153, 153);
    font-weight: 400;
}
.countDownBox {
   margin: 2vw 0 3vw 0;
}
.countDownBox .text {
    color: #75460A;
    font-weight: 500;
}

.main .themeTitle{
    display: flex;
    margin-bottom: 5vw;
    margin-top: 8vw;
    padding: 0 5vw;
}
.main .center {
    flex: 1;
    margin: 0 2vw;
}
.main .left, .main .right{
    flex: 1;
    height: 4vw;
}

.main .left {
    margin-right: -7vw;
}
.main .right {
    margin-left: -7vw;
}

.main .leadItem {
    display: flex;
}

.leadItem .stepItem {
    flex: 1;
    padding: 0 2vw;
}

.leadItem .stepItem image {
    width: 100%;
}

.orderDetail .leadItem .subTitle {
    color: rgb(26, 26, 26);
    font-size: 3.2vw;
    line-height: 4.8vw;
}

.colorYellow {
    color: rgb(215, 179, 101);
}

.orderDetailInfo {
    width:88vw;
    padding:3vw;
    background: rgb(255, 255, 255);
    margin: 0 auto 2.66667vw;
    border-radius: 2.66667vw;
}

.orderDetailInfo .header  {
    display: flex;
}

.orderDetailInfo .header .left{
  
}

.orderDetailInfo .header .right{
     color: rgb(216, 177, 93);
     text-align: right;
     flex: 1;
}

.orderDetail .addressBox  {
   overflow: hidden;
}

.orderDetail .addressBox .address {
    padding: 2vw 0;
    border-bottom: 1px solid rgb(229, 229, 229);
    display: flex;
    align-items:center;
    margin-top: 8vw;
}
.orderDetail .addressBox .address .iconBox{
    width: 12vw;
    text-align: center;
}

.orderDetail .addressBox .address .iconBox .locationIcon {
    width: 5vw;
    margin: 0 auto;

}
.orderDetail .addressBox .address .textBox{
    flex: 2;
}
.orderDetail .addressBox .address .textBox .area, .orderDetail .addressBox .address .textBox .street {
    font-size: 3vw;
    color: rgb(153, 153, 153);
}
 
 .orderDetail .goodInfoBox {
    padding:4vw 0;
}

.orderDetail .goodInfoBox .sectionTitle{
    height: 5vw;
    line-height: 5vw;
    font-size: 4vw;
    color: rgb(26, 26, 26);
    margin-bottom:5vw;
}

.orderDetail .recieveTime {
    font-size: 3.2vw;
    color: rgb(153, 153, 153);
    padding: 4vw 0;
    padding-right: 4vw;
    padding-left: 6.66667vw;
    text-align: left;
}

.addressBoxShow {
    animation: addressBoxShowAni .5s forwards;
}

.addressBoxHide {
    animation: addressBoxHideAni .5s forwards;
}

@keyframes addressBoxShowAni{
    0%{
        height: 0;
    }
    100%{
        height: 85vw;
    }
}
@keyframes addressBoxHideAni{
    0%{
        height:85vw;
    }
    100%{
        height: 0;
    }
}

.contactBox {
    height: 15vw;
    width: 6vw;
    position: fixed;
    right: 0;
    top: 40vw;
    background-size:100% 100%;
   
}


