.footerBox{
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100vw;
    box-sizing: border-box;
    text-align: center;
    background: rgb(14, 10, 9);
    transition: all .5s ease;
}

.footerBox .line{
    width: 100%;
    height: 1px;
    background: linear-gradient(90deg,rgb(53, 49, 45),rgb(241, 211, 174),rgb(53, 49, 45));
}
.footerBox .label{
    width: 44.8vw;
    height: 8.8vw;
    position: absolute;
    right: 5.33333vw;
    top: -7.46667vw;
    animation: bounce-down 1.5s linear infinite;
}

.footerBox .footerTop {
    width: 94.66667vw;
    height: 13.33333vw;
    margin: 2.13333vw auto 2.13333vw;
    background: linear-gradient(-180deg,rgb(237, 202, 126),rgb(216, 168, 91));
    border-radius: 13.33333vw;
    line-height: 13.33333vw;
    color: rgb(117, 70, 10);
    font-size: 4.26667vw;
}

@keyframes bounce-down {
    25% {-webkit-transform: translateY(-5px);}
    50%, 100% {-webkit-transform: translateY(0);}
    75% {-webkit-transform: translateY(5px);}
}

.contactBox {
